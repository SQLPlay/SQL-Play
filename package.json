    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1",
    "webdriverio": "^7.20.7"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "reactNativeVersion": "^0.69.0",
    "kitType": "app",
    "capabilities": [
      "animation",
      "babel-preset-react-native",
      "core",
      "core-android",
      "core-ios",
      "filesystem",
      "gestures",
      "react",
      "react-test-renderer",
      "safe-area",
      "sqlite",
      "svg"
    ]
  }
